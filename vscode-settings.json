{
    "[python]": {
        "editor.rulers": [
            {
                "column": 80,
                "color": "#c3c30090"
            },
            {
                "column": 120,
                "color": "#c4000090"
            }
        ],
        "explicitFolding.rules": [
            {
                "beginRegex": "\"\"\"|'''",
                "endRegex": "\"\"\"|'''",
                "autoFold": true
            },
            {
                "indentation": true,
                "offSide": true
            }
        ],
    },
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "autoDocstring.docstringFormat": "google",
    "autoDocstring.startOnNewLine": false,
    "autoDocstring.customTemplatePath": "C:\\Users\\*username*\\.vscode\\orbital_docstring.mustache",
    "code-runner.clearPreviousOutput": true,
    "code-runner.executorMap": {
        "python": "$pythonPath -u $fullFileName"
    },
    "code-runner.runInTerminal": true,
    "code-runner.saveFileBeforeRun": true,
    "code-runner.showExecutionMessage": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.cursorSurroundingLines": 10,
    "editor.defaultFoldingRangeProvider": "zokugun.explicit-folding",
    "editor.detectIndentation": true,
    "editor.foldingImportsByDefault": false,
    "editor.foldingStrategy": "auto",
    "editor.fontFamily": "'FiraCode NF', 'Fira Code', 'Cascadia Code PL', Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 9,
    "editor.formatOnSave": false,
    "editor.insertSpaces": true,
    "editor.minimap.enabled": false,
    "editor.multiCursorPaste": "full",
    "editor.lineNumbers": "relative",
    "editor.linkedEditing": true,
    "editor.showFoldingControls": "always",
    "editor.suggestSelection": "first",
    "editor.tabSize": 4,
    "explorer.openEditors.visible": 5,
    "explorer.confirmDelete": false,
    "files.associations": {
        "*.ino": "cpp"
    },
    "files.exclude": {
        "**/.git": false,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true
    },
    "gotoNextPreviousMember.symbolKinds": [
        "class",
        "function",
        "method",
    ],
    "jupyter.interactiveWindow.creationMode": "perFile",
    "markdownlint.config": {
        "MD033": false
    },
    "module-templates.engine": "handlebars",
    "module-templates.templateFiles": [
        "C:\\Users\\*username*\\.vscode\\template_test_script_basic.json",
        "C:\\Users\\*username*\\.vscode\\template_test_script_expert.json",
        "C:\\Users\\*username*\\.vscode\\template_test_script_intermediate.json",
        "C:\\Users\\*username*\\.vscode\\template_test_script_hafiz.json"
    ],
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "python.analysis.diagnosticMode": "openFilesOnly",
    "python.languageServer": "Pylance",
    "python.formatting.blackArgs": [
        "--line-length=79"
    ],
    "python.formatting.autopep8Args": [
        "--max-line-length",
        "79",
        "--experimental"
    ],
    "python.formatting.autopep8Path": "C:\\Users\\*username*\\.pyenv\\pyenv-win\\versions\\3.11.4\\Scripts\\autopep8.exe",
    "python.formatting.blackPath": "C:\\Users\\*username*\\.pyenv\\pyenv-win\\versions\\3.11.4\\Scripts\\black.exe",
    "python.formatting.provider": "black",
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": false,
    "python.linting.flake8Path": "C:\\Users\\*username*\\.pyenv\\pyenv-win\\versions\\3.11.4\\Scripts\\flake8.exe",
    "python.linting.flake8Args": [
        "--ignore=E501"
    ],
    "python.venvPath": "~/.virtualenvs",
    "ruff.args": [
        "--ignore=501"
    ],
    "search.quickOpen.includeHistory": false,
    "sourcery.token": "user_AmrM0pIFJa6n4_3zbAYqBoPuiMkykg4GF_5likNB_eeUdUYLHEplQ8awMJQ",
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.cwd": "${fileDirname}",
    "terminal.integrated.fontFamily": "'FiraCode NF','Cascadia Code', Consolas, 'Courier New', monospace",
    "terminal.integrated.fontSize": 10,
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [],
            "icon": "terminal-cmd"
        },
        "Git Bash": {
            "source": "Git Bash"
        }
    },
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "todohighlight.isCaseSensitive": true,
    "todohighlight.isEnable": true,
    "todohighlight.keywords": [
        {
            "isWholeLine": true,
            "text": "TODO:",
            "color": "black"
        },
        {
            "isWholeLine": true,
            "text": "FIXME:"
        },
        {
            "isWholeLine": true,
            "text": "HACK:",
            "color": "black",
            "backgroundColor": "#FF99FF"
        },
    ],
    "window.autoDetectColorScheme": true,
    "workbench.editor.pinnedTabSizing": "compact",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "workbench.iconTheme": "vscode-icons",
    "workbench.list.horizontalScrolling": true,
    "workbench.startupEditor": "none",
    "vim.leader": "<space>",
    "vim.easymotion": true,
    "vim.surround": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "<leader>",
                "y"
            ],
            "after": [
                "\"",
                "a",
                "y",
                "<enter>"
            ]
        },
        {
            "before": [
                "<leader>",
                "p"
            ],
            "after": [
                "\"",
                "a",
                "p"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "y"
            ],
            "after": [
                "\"",
                "a",
                "y",
                "<enter>"
            ]
        }
    ],
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false,
        "<C-w>": false
    },
    "vim.smartRelativeLine": false,
    "vsicons.dontShowNewVersionMessage": true,
    "workbench.colorTheme": "Default Dark Modern",
    "workbench.editor.enablePreview": false,
}
