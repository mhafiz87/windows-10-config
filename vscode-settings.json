{
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "autoDocstring.docstringFormat": "google",
    "autoDocstring.startOnNewLine": false,
    "autoDocstring.customTemplatePath": "orbital.mustache",
    "code-runner.clearPreviousOutput": true,
    "code-runner.executorMap": {
        "python": "$pythonPath -u $fullFileName"
    },
    "code-runner.runInTerminal": true,
    "code-runner.saveFileBeforeRun": true,
    "code-runner.showExecutionMessage": false,
    "editor.bracketPairColorization.enabled": true,
    "editor.codeActionsOnSave": {
        "source.organizeImports": true
    },
    "editor.cursorSurroundingLines": 10,
    "editor.detectIndentation": false,
    "editor.fontFamily": "'FiraCode NF', 'Fira Code', 'Cascadia Code PL', Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 12,
    "editor.formatOnSave": true,
    "editor.guides.bracketPairs": "active",
    "editor.guides.bracketPairsHorizontal": "active",
    "editor.insertSpaces": true,
    "editor.minimap.enabled": false,
    "editor.lineNumbers": "on",
    "editor.linkedEditing": true,
    "editor.showFoldingControls": "always",
    "editor.suggestSelection": "first",
    "editor.tabSize": 4,
    "explorer.openEditors.visible": 5,
    "explorer.confirmDelete": false,
    "files.associations": {
        "*.ino": "cpp"
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true
    },
    "files.trimTrailingWhitespace": true,
    "gitlens.mode.active": "review",
    "gotoNextPreviousMember.symbolKinds": [
        "class",
        "function",
        "method",
    ],
    "jupyter.interactiveWindowMode": "perFile",
    "jupyter.askForKernelRestart": false,
    "liveServer.settings.donotShowInfoMsg": true,
    "markdownlint.config": {
        "MD033": false,
        "MD033": false
    },
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "prettier.tabWidth": 4,
    "python.languageServer": "Pylance",
    "python.formatting.blackArgs": [
        "--line-length=140"
    ],
    "python.formatting.blackPath": "${env:LOCALAPPDATA}/Programs/Python/Python39/Scripts/black.exe",
    "python.formatting.provider": "black",
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": true,
    "python.linting.flake8Path": "${env:LOCALAPPDATA}/Programs/Python/Python39/Scripts/flake8.exe",
    "python.linting.flake8Args": [
        "--ignore=E501,W503"
    ],
    "python.venvPath": "${env:USERPROFILE}/.virtualenvs",
    "search.quickOpen.includeHistory": false,
    "sourcery.token": "user_AmrM0pIFJa6n4_3zbAYqBoPuiMkykg4GF_5likNB_eeUdUYLHEplQ8awMJQ",
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.cwd": "${fileDirname}",
    "terminal.integrated.fontFamily": "'FiraCode NF','Cascadia Code', Consolas, 'Courier New', monospace",
    "terminal.integrated.fontSize": 11,
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [],
            "icon": "terminal-cmd"
        },
        "Git Bash": {
            "source": "Git Bash"
        }
    },
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "todohighlight.isCaseSensitive": true,
    "todohighlight.isEnable": true,
    "todohighlight.keywords": [
        {
            "isWholeLine": true,
            "text": "TODO:",
            "color": "black"
        },
        {
            "isWholeLine": true,
            "text": "FIXME:"
        }
    ],
    "typescript.suggest.paths": false,
    "window.autoDetectColorScheme": true,
    "window.zoomLevel": 0,
    "workbench.colorTheme": "Default Dark+",
    "workbench.editor.pinnedTabSizing": "compact",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "workbench.iconTheme": "vscode-icons",
    "workbench.list.horizontalScrolling": true,
    "workbench.startupEditor": "none",
    "vim.easymotion": true,
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false,
        "<C-w>": false
    },
    "vim.hlsearch": true,
    "vim.incsearch": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.leader": "<space>",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "<leader>",
                "y"
            ],
            "after": [
                "\"",
                "a",
                "y",
                "<enter>"
            ]
        },
        {
            "before": [
                "<leader>",
                "p"
            ],
            "after": [
                "\"",
                "a",
                "p"
            ]
        }
    ],
    "vim.smartRelativeLine": true,
    "vim.surround": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "y"
            ],
            "after": [
                "\"",
                "a",
                "y",
                "<enter>"
            ]
        }
    ],
    "vsicons.dontShowNewVersionMessage": true,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "workbench.editor.enablePreview": true,
}
